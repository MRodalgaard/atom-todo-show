{
  "name": "todo-show",
  "version": "1.11.0",
  "main": "./lib/show-todo",
  "description": "Finds all the TODOs, FIXMEs, CHANGEDs, etc. in your project.",
  "activationCommands": {
    "atom-workspace": [
      "todo-show:find-in-workspace",
      "todo-show:find-in-project",
      "todo-show:find-in-open-files",
      "todo-show:find-in-active-file"
    ]
  },
  "repository": "https://github.com/mrodalgaard/atom-todo-show",
  "license": "MIT",
  "engines": {
    "atom": ">1.17.0"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.3",
    "fs-plus": "2.x",
    "sortablejs": "^1.4.2",
    "temp": "^0.8.3",
    "underscore-plus": "^1.6.6"
  }
}
